{
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "trailingComma": "all",
    "insertPragma": false,
    "singleAttributePerLine": false,
    "bracketSameLine": false,
    "jsxBracketSameLine": true,
    "jsxSingleQuote": false,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 120,
    "vueIndentScriptAndStyle": false
}